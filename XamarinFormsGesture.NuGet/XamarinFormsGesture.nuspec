<?xml version="1.0"?>
<package >
  <metadata>
    <id>Vapolia.XamarinFormsGesture</id>
    <version>0.0.0</version>

    <title>XamarinFormsGesture</title>
    <authors>Benjamin Mayrargue</authors>
    <owners>Vapolia SARL</owners>
    <description>
      Bring all platform gestures to Xamarin Forms on Android and iOS. Add this nuget to your Netstandard, Android, iOS and UWP projects.
    </description>
    <releaseNotes>
      3.1.0: xamarin forms 3.6. Double tap command.
      3.0.0: netstandard 2.0, xamarin forms 3. UWP requires fall creator update.
      2.0.0: netstandard 1.4, xamarin forms 2.4
      1.1.1: add universal windows implementation
      1.1.0: add pan gesture
      1.0.1: fix ios version for XamForms DependencyService (changed internal to public in class definition)
    </releaseNotes>
    <summary>Bring all gestures to Xamarin Forms</summary>
    <language>en-US</language>
    <projectUrl>https://github.com/softlion/XamarinFormsGesture/</projectUrl>
    <iconUrl>https://github.com/softlion/XamarinFormsGesture/favicon.ico</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/softlion/XamarinFormsGesture/license</licenseUrl>
    <copyright>Copyright 2016-2018 vapolia.fr</copyright>

    <dependencies>
      <group>
        <dependency id="Xamarin.Forms" version="3.6.0" />
      </group>
      <group targetFramework="uap10.0.16299">
      </group>
    </dependencies>
    <references></references>
    <tags>xamarin forms gesture tap swipe pan tap doubletap</tags>
  </metadata>
  <files>
    <!-- forms droid -->
    <file src="..\XamarinFormsGesture.Droid\bin\Release\XamarinFormsGesture.Droid.dll" target="lib\MonoAndroid1.0\XamarinFormsGesture.Droid.dll" />
    <file src="..\XamarinFormsGesture\bin\Release\netstandard2.0\XamarinFormsGesture.dll" target="lib\MonoAndroid1.0\XamarinFormsGesture.dll" />

    <!-- forms ios -->
    <file src="..\XamarinFormsGesture.Ios\bin\Release\XamarinFormsGesture.Ios.dll" target="lib\Xamarin.iOS1.0\XamarinFormsGesture.Ios.dll" />
    <file src="..\XamarinFormsGesture\bin\Release\netstandard2.0\XamarinFormsGesture.dll" target="lib\Xamarin.iOS1.0\XamarinFormsGesture.dll" />

    <!-- forms shared -->
    <file src="..\XamarinFormsGesture\bin\Release\netstandard2.0\XamarinFormsGesture.dll" target="lib\netstandard2.0\XamarinFormsGesture.dll" />

    <!-- universal (win10) -->
    <file src="..\XamarinFormsGesture.Uw\bin\Release\XamarinFormsGesture.Uw.dll" target="lib\uap10.0\XamarinFormsGesture.Uw.dll" />
    <file src="..\XamarinFormsGesture.Uw\bin\Release\XamarinFormsGesture.Uw.pri" target="lib\uap10.0\XamarinFormsGesture.Uw.pri" />
    <file src="..\XamarinFormsGesture\bin\Release\netstandard2.0\XamarinFormsGesture.dll" target="lib\uap10.0\XamarinFormsGesture.dll" />
  </files>
</package>
